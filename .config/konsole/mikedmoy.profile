[Appearance]
AntiAliasFonts=true
BoldIntense=true
ColorScheme=Linux
DimmValue=128
Font=Cascadia Mono PL,12,-1,5,50,0,0,0,0,0
LineSpacing=0
TabColor=0,255,0
UseFontLineChararacters=false

[Cursor Options]
CursorShape=0
CustomCursorColor=255,255,255
CustomCursorTextColor=0,0,0
UseCustomCursorColor=false

[Encoding Options]
DefaultEncoding=UTF-8

[General]
